<h1>Listing Memos</h1>
<%= link_to 'New Memo', new_memo_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @memos.each do |memo| %>
    <tr>
      <td><%= memo.title %></td>
      <td><%= memo.text %></td>
      <td><%= link_to 'Show', memo_path(memo) %></td>
    </tr>
  <% end %>
</table>
